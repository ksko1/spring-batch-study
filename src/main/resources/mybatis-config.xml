<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <settings>
    <setting name="mapUnderscoreToCamelCase" value="true" /> <!-- 데이터베이스 컬럼명의 언더스코어(_)를 자바 객체의 카멜케이스 프로퍼티명으로 자동 매핑 -->
    <setting name="multipleResultSetsEnabled" value="true" /> <!-- 여러 개의 ResultSet을 반환하는 저장 프로시저 호출 지원 활성화 -->
    <setting name="callSettersOnNulls" value="true"/> <!-- 가져온 값이 null일 때도 setter나 맵의 put 메소드를 호출하도록 설정 -->
  </settings>
</configuration>